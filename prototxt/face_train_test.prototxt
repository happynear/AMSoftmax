  type: "ImageData"
  top: "data"
  top: "label"
  include { 
    phase: TRAIN
  }
  transform_param {
    mean_value: 128
    scale: 0.0078125
    mirror: true
  }
  image_data_param {
    source: "C:/datasets/CASIA-maxpy-clean-aligned-96/list.txt"
    root_folder: "C:/datasets/CASIA-maxpy-clean-aligned-96/"
    batch_size: 256 
    shuffle: true
  }
}
